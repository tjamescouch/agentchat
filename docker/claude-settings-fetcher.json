{
  "permissions": {
    "allow": [
      "WebSearch(*)",
      "WebFetch(*)",
      "Read(*)",
      "Write(*)",
      "mcp__agentchat__*"
    ],
    "deny": [
      "Bash(*)",
      "Edit(*)"
    ],
    "autoApprove": true
  },
  "mcpServers": {
    "agentchat": {
      "command": "npx",
      "args": ["-y", "@tjamescouch/agentchat-mcp"],
      "env": {
        "AGENTCHAT_PUBLIC": "true"
      }
    }
  },
  "dangerouslyDisableSandbox": true
}

{
  "name": "@tjamescouch/agentchat",
  "version": "0.26.0",
  "description": "WebSocket protocol for real-time AI agent coordination â€” IRC for machines",
  "main": "dist/lib/client.js",
  "types": "dist/lib/client.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/lib/client.d.ts",
      "import": "./dist/lib/client.js"
    },
    "./server": {
      "types": "./dist/lib/server.d.ts",
      "import": "./dist/lib/server.js"
    },
    "./protocol": {
      "types": "./dist/lib/protocol.d.ts",
      "import": "./dist/lib/protocol.js"
    },
    "./types": {
      "types": "./dist/lib/types.d.ts",
      "import": "./dist/lib/types.js"
    },
    "./lib/daemon.js": {
      "types": "./dist/lib/daemon.d.ts",
      "import": "./dist/lib/daemon.js"
    },
    "./lib/identity.js": {
      "types": "./dist/lib/identity.d.ts",
      "import": "./dist/lib/identity.js"
    },
    "./lib/jitter.js": {
      "types": "./dist/lib/jitter.d.ts",
      "import": "./dist/lib/jitter.js"
    },
    "./lib/protocol.js": {
      "types": "./dist/lib/protocol.d.ts",
      "import": "./dist/lib/protocol.js"
    }
  },
  "bin": {
    "agentchat": "./dist/bin/agentchat.js"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "Dockerfile"
  ],
  "scripts": {
    "build": "npx tsc",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "prepublishOnly": "npm run rebuild",
    "start": "node dist/bin/agentchat.js serve",
    "dev": "npx tsc --watch",
    "test": "npm run build && node --test 'test/*.test.js'",
    "test:integration": "npm run build && node --test test/*.integration.test.js",
    "test:all": "npm run build && node --test 'test/**/*.test.js'",
    "test:watch": "node --test --watch 'test/*.test.js'",
    "typecheck": "npx tsc --noEmit",
    "lint": "knip",
    "preflight": "npm run typecheck && npm run lint && npm test"
  },
  "keywords": [
    "ai",
    "agents",
    "multi-agent",
    "chat",
    "irc",
    "llm",
    "websocket",
    "coordination",
    "protocol",
    "mcp",
    "claude",
    "real-time"
  ],
  "author": {
    "name": "James Couch",
    "url": "https://github.com/tjamescouch"
  },
  "license": "MIT",
  "homepage": "https://github.com/tjamescouch/agentchat#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tjamescouch/agentchat.git"
  },
  "bugs": {
    "url": "https://github.com/tjamescouch/agentchat/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/tjamescouch"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@akashnetwork/akashjs": "^0.11.1",
    "@cosmjs/proto-signing": "^0.32.4",
    "@cosmjs/stargate": "^0.32.4",
    "commander": "^12.0.0",
    "js-yaml": "^4.1.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.0",
    "@types/ws": "^8.5.10",
    "knip": "^5.83.0",
    "typescript": "^5.3.3"
  }
}

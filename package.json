{
  "name": "@tjamescouch/agentchat",
  "version": "0.22.1",
  "description": "Real-time IRC-like communication protocol for AI agents",
  "main": "dist/lib/client.js",
  "types": "dist/lib/client.d.ts",
  "files": [
    "dist/",
    "README.md",
    "Dockerfile"
  ],
  "bin": {
    "agentchat": "./dist/bin/agentchat.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "start": "node dist/bin/agentchat.js serve",
    "dev": "tsc --watch",
    "test": "npm run build && node --test 'test/*.test.js'",
    "test:integration": "npm run build && node --test test/*.integration.test.js",
    "test:all": "npm run build && node --test test/*.test.js",
    "typecheck": "tsc --noEmit",
    "knip": "knip"
  },
  "keywords": [
    "ai",
    "agents",
    "chat",
    "irc",
    "llm",
    "communication",
    "protocol"
  ],
  "author": "James Couch",
  "license": "MIT",
  "homepage": "https://github.com/tjamescouch/agentchat#readme",
  "dependencies": {
    "@akashnetwork/akashjs": "^0.11.1",
    "@cosmjs/proto-signing": "^0.32.4",
    "@cosmjs/stargate": "^0.32.4",
    "commander": "^12.0.0",
    "js-yaml": "^4.1.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.0",
    "@types/ws": "^8.5.10",
    "knip": "^5.83.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tjamescouch/agentchat.git"
  },
  "bugs": {
    "url": "https://github.com/tjamescouch/agentchat/issues"
  },
  "type": "module"
}
